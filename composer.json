{
    "name": "thafanseid/blade-zwe-icons",
    "description": "A package to easily make use of ZweIcon in your Laravel Blade views.",
    "keywords": ["Blade", "ZweIcon","Zwe Icon", "Laravel"],
    "homepage": "https://github.com/blade-organization/blade-heroicons",
    "license": "MIT",
    "authors": [
        {
            "name": "Soe Moe Thu",
            "email": "soemoethu.dev@gmail.com"
        }
    ],
    "require": {
        "php": "~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.4",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Thafanseid\\ZweIcon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thafanseid\\ZweIcon\\BladeZweIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
